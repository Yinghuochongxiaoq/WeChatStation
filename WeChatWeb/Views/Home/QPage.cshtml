@model dynamic

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Swiper demo</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
    <!-- Link Swiper's CSS -->
    <link rel="stylesheet" href="../../Static/009/css/swiper.min.css">
    <!-- Demo styles -->
    <link href="~/Static/009/css/hunsha.css" rel="stylesheet" />
</head>
<body>
    <!-- Swiper -->
    <div class="swiper-container">
        <div class="swiper-wrapper">
            <div class="swiper-slide pic0">
                <section class="fn-audio">
                    <div class="btn">
                        <p class="btn_audio">
                            <span class="css_sprite01 audio_close"></span>
                            <span class="css_sprite01 audio_open"></span>
                        </p>
                        <audio id="car_audio" controls preload="preload">
                            <source src="~/Static/009/sounds/music.mp3" type="audio/mpeg">您的浏览器不支持HTML5音频格式
                        </audio>
                    </div>
                </section>
            </div>
            <div class="swiper-slide">
                <div class="plate pic2 plate01">
                </div>
                <div class="plate pic3">
                </div>
            </div>
            <div class="swiper-slide pic5"></div>
            <div class="swiper-slide">
                <div class="plate pic7 plate01">
                </div>
                <div class="plate pic8">
                </div>
            </div>
            <div class="swiper-slide pic6"></div>
            <div class="swiper-slide">
                <div class="plate pic9">
                </div>
                <div class="tableWrap tableWrap-1">
                    <p class="submit submit-custom btn-boder-color">
                        <input type="text" class="form-control" name="" id="barrage_content" placeholder="送祝福啦">
                        <input type="button" class="btn btn-primary" id="submit_barraget" value="送祝福">
                    </p>
                    <div class="share-content">
                        <div class="m-spopup"></div>
                    </div>
                </div>
                <div class="tableWrap tableWrap-2"></div>
            </div>
            <div class="swiper-slide">
                <div class="m-map">
                    <div id="ylMap" class="ylMap"></div>
                    <div class="mapVal ">
                        <input class="address" type="hidden" value="{&#39;sign_name&#39;:&#39;&#39;,&#39;contact_tel&#39;:&#39;18883257311&#39;,&#39;address&#39;:&#39;重庆市忠县花桥镇天井村洞口沟&#39;}">
                        <input class="latitude" type="hidden" value="30.3431060041">
                        <input class="longitude" type="hidden" value="107.6910631654">
                    </div>
                    <div id="transit_result"></div>
                    <div class="tit">
                        <p><a href="#"><span class="css_sprite01"></span>重庆市忠县花桥镇天井村</a></p>
                        <a class="close_map css_sprite01" href="javascript:void(0)"></a>
                    </div>
                </div>
            </div>
        </div>
        <!-- Add Pagination -->
        <div class="swiper-pagination"></div>
    </div>
    <section class="u-arrow">
        <img src="../../Static/009/images/btn01_arrow.png">
    </section>

    <!-- Swiper JS -->
    <script src="../../Static/009/js/swiper.min.js"></script>
    <script type="text/javascript" src="../../Static/009/js/jquery-1.8.2.min.js"></script>
    <script type="text/javascript" src="../../Static/009/js/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="../../Static/009/js/ylMap.js"></script>
    <!-- Initialize Swiper -->
    <script src="~/Static/009/js/hunsha.js"></script>
</body>
</html>